{
  "name": "@dl-tech/langchain",
  "version": "1.0.0",
  "description": "LangChain utilities for NestJS",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./src/index.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "peerDependencies": {
    "@langchain/core": ">=1.0.0",
    "@nestjs/common": ">=10.0.0",
    "class-transformer": ">=0.5.0",
    "class-validator": ">=0.14.0",
    "langchain": ">=0.3.0",
    "reflect-metadata": ">=0.1.0",
    "rxjs": ">=7.0.0",
    "zod": ">=3.0.0"
  },
  "dependencies": {
    "fluent-json-schema": "^6.0.0",
    "partial-json": "^0.1.7"
  },
  "devDependencies": {
    "@langchain/core": "^1.1.2",
    "@nestjs/common": "^11.1.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "langchain": "^1.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "zod": "^3.24.0"
  }
}
